{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ESNext",
    "lib": ["ESNext"],
    "types": ["@types/node"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "noEmit": true
  },
  "include": ["./src"]
}
